+  /**
+   * Log all visible items in the list for debugging
+   */
+  private void logVisibleItems(ReactScrollView scrollView) {
+    if (scrollView == null) {
+      return;
+    }
+
+    int childCount = getChildCount();
+    Log.d("A11Y_DEBUG", "=== VISIBLE ITEMS DETECTION ===");
+    Log.d("A11Y_DEBUG", "ScrollView scrollY: " + scrollView.getScrollY() + ", height: " + scrollView.getHeight());
+    Log.d("A11Y_DEBUG", "Total childCount: " + childCount);
+
+    int visibleCount = 0;
+    int messageCount = 0;
+    
+    for (int i = 0; i < childCount; i++) {
+      View child = getChildAt(i);
+      if (child != null) {
+        boolean isShown = child.isShown();
+        boolean isVisible = child.getVisibility() == View.VISIBLE;
+        boolean isMessage = isMessageView(child);
+        
+        String testId = "";
+        Object testIdTag = child.getTag(R.id.react_test_id);
+        if (testIdTag instanceof String) {
+          testId = (String) testIdTag;
+        }
+
+        String desc = child.getContentDescription() != null ? child.getContentDescription().toString() : "null";
+        String className = child.getClass().getSimpleName();
+
+        if (isShown && isVisible) {
+          visibleCount++;
+          if (isMessage) {
+            messageCount++;
+          }
+          Log.d("A11Y_DEBUG", String.format(
+            "  VISIBLE[%d]: %s - id=%d, testID=%s, className=%s, desc=%s",
+            i, isMessage ? "MESSAGE" : "NOT MESSAGE", child.getId(), testId, className, desc
+          ));
+        }
+      }
+    }
+
+    Log.d("A11Y_DEBUG", "Summary - Visible items: " + visibleCount + " (messages: " + messageCount + ", non-messages: " + (visibleCount - messageCount) + ")");
